        "item": [
          {
            "linkId": "suspectInfluenza",
            "text": "Bệnh nhân có biểu hiện triệu chứng viêm đường hô hấp cấp hoặc có yếu tố dịch tễ liên quan?",
            "type": "boolean"
          },
          {
            "linkId": "riskAssessment",
            "text": "Người bệnh có yếu tố nguy cơ diễn biến nặng?",
            "type": "boolean"
          },
          {
            "linkId": "severeCase",
            "text": "Người bệnh có biểu hiện nặng (khó thở, thiếu oxy, suy cơ quan, shock)?",
            "type": "boolean"
          },
          {
            "linkId": "diagnosticTests",
            "text": "Chọn xét nghiệm sàng lọc/khẳng định",
            "type": "choice",
            "answerOption": [
              {"valueCoding": {"code": "rapid", "display": "Test nhanh"}},
              {"valueCoding": {"code": "pcr", "display": "RT-PCR"}}
            ]
          },
          {
            "linkId": "considerAntiviral",
            "text": "Người bệnh nghi ngờ/khẳng định cúm nặng hoặc có yếu tố nguy cơ?",
            "type": "boolean"
          },
          {
            "linkId": "mildCaseManagement",
            "text": "Người bệnh cúm nhẹ - điều trị hỗ trợ",
            "type": "display"
          },
          {
            "linkId": "outpatientSupport",
            "text": "Điều trị hỗ trợ ngoại trú",
            "type": "display"
          },
          {
            "linkId": "interpretation",
            "text": "Kết quả xét nghiệm dương tính?",
            "type": "boolean"
          },
          {
            "linkId": "confirmCase",
            "text": "Ca bệnh khẳng định - phân mức độ và điều trị",
            "type": "display"
          },
          {
            "linkId": "differentialDiagnosis",
            "text": "Cân nhắc chẩn đoán phân biệt nếu xét nghiệm âm tính",
